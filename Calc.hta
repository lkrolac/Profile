   <script language=javascript>
var winWidth=960; // ширина окна
var winHeight=600; // высота окна
// изменяем размер
window.resizeTo(winWidth, winHeight);

// окно в центр экрана
var winPosX=screen.width/2-winWidth/2;
var winPosY=screen.height/2-winHeight/2;
window.moveTo(winPosX, winPosY);
</script>

<script>
  $(function() {
    $( "#slider-range-min" ).slider({
      range: "min",
      value: 2,
      min: 1,
      max: 40,
      slide: function( event, ui ) {
        $( "#inpZ" ).val( ui.value );
      }
    });
    $( "#inpZ" ).val( $( "#slider-range-min" ).slider( "value" ) );
  });

  $(function() {
    $( "#slider-range-min2" ).slider({
      range: "min",
      value: 22,
      min: 1,
      max: 1200,
      slide: function( event, ui ) {
        $( "#inpA" ).val( ui.value );
      }
    });
    $( "#inpA" ).val( $( "#slider-range-min2" ).slider( "value" ) );
  });

    $(function() {
    $( "#slider-range-min3" ).slider({
      range: "min",
      value: 20,
      min: 1,
      max: 100,
      slide: function( event, ui ) {
        $( "#inpB" ).val( ui.value );
      }
    });
    $( "#inpB" ).val( $( "#slider-range-min3" ).slider( "value" ) );
  });

    $(function() {
    $( "#slider-range-min4" ).slider({
      range: "min",
      value: 20,
      min: 1,
      max: 1000,
      slide: function( event, ui ) {
        $( "#inpX" ).val( ui.value );
      }
    });
    $( "#inpX" ).val( $( "#slider-range-min4" ).slider( "value" ) );
  });

    $(function() {
    $( "#slider-range-min5" ).slider({
      range: "min",
      value: 20000,
      min: 0,
      max: 1000000,
      step: 1000,
      slide: function( event, ui ) {
        $( "#inpY" ).val( ui.value );
      }
    });
    $( "#inpY" ).val( $( "#slider-range-min5" ).slider( "value" ) );
  });
  </script>

   <style type="text/css">
   body {
    font-family: "Open Sans";
   }
   input {
        border: 2px solid #d3d3d3;
        padding: 5px;
        font-family: "Open Sans";
    }
    button {
      background: #ba4242;
      color: #fff;
      display: inline-block;
      padding: 8px 30px 8px;
      border: none;
      font-family: "Open Sans";
      font-size: 100%;
      outline: none;    
